		<parameter name="validasi" class="java.lang.String"/>
		<queryString>
			<![CDATA[select sum(payment_amount), TRANS_DATE, DESCRIPTION, PAYMENT_METHOD FROM (
SELECT a.payment_amount, a.TRANS_DATE, j.description AS DESCRIPTION, d.description AS PAYMENT_METHOD  
FROM t_pos_bill_payment_detail a 
LEFT JOIN T_POS_BILL_PAYMENT b ON a.BILL_NO=b.BILL_NO AND a.POS_CODE=b.POS_CODE AND (b.payment_method_code <> 'CSH' AND b.payment_method_code <> 'VCR') 
LEFT JOIN M_pos_STAFF c ON a.USER_UPD = c.STAFF_POS_CODE 
LEFT JOIN M_GLOBAL d ON d.CODE =a.PAYMENT_METHOD_CODE AND d.cond='PAY_METHOD' 
LEFT JOIN M_PAYMENT_METHOD e ON a.PAYMENT_METHOD_CODE=e.PAYMENT_METHOD_CODE AND e.PAYMENT_TYPE_CODE='DBT' AND e.status='A'
LEFT JOIN M_GLOBAL f ON f.CODE =e.PAYMENT_TYPE_CODE 
LEFT JOIN T_POS_BILL G ON a.POS_CODE=g.POS_CODE AND a.BILL_NO=g.BILL_NO AND g.delivery_status='CLS'  
LEFT JOIN M_pos_STAFF h ON b.PAYMENT_MANAGER_CODE = h.STAFF_POS_CODE
LEFT JOIN M_OUTLET i ON a.OUTLET_CODE = i.OUTLET_CODE 
LEFT JOIN M_GLOBAL j ON g.order_type = j.code AND j.cond = 'ORDER_TYPE'
WHERE a.TRANS_DATE BETWEEN $P{transDate1} AND $P{transDate2} 
		AND g.POS_CODE BETWEEN $P{posCode1} AND $P{posCode2}
        AND g.CASHIER_CODE BETWEEN $P{cashierCode1} AND $P{cashierCode2}
        AND g.BILL_TIME BETWEEN $P{billTime1} AND $P{billTime2}
        AND g.SHIFT_CODE BETWEEN $P{shiftCode1} AND $P{shiftCode2} AND a.PAYMENT_METHOD_CODE <> 'VCR' AND (
		    ($P{validasi} = 'Manual' AND b.PAYMENT_MANAGER_CODE NOT IN (NULL, '', ' '))
		    OR
		    ($P{validasi} = 'Otomatis' AND b.PAYMENT_MANAGER_CODE IN (NULL, '', ' '))
		    OR 
		    ($P{validasi} NOT IN ('Manual', 'Otomatis'))
		    OR 
		    $P{validasi} IS NULL 
		    )
)
GROUP BY TRANS_DATE,DESCRIPTION, PAYMENT_METHOD
ORDER BY TRANS_DATE, PAYMENT_METHOD ASC]]>
		</queryString>
