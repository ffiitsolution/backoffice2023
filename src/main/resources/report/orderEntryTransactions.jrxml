	<queryString>
		<![CDATA[SELECT
	a.ITEM_CODE,
	b.ITEM_DESCRIPTION,
	b.CONV_WAREHOUSE,
	b.UOM_PURCHASE,
	b.UOM_WAREHOUSE,
	a.QTY_1,
	a.CD_UOM_1,
	a.QTY_2,
	a.CD_UOM_2,
	a.TOTAL_QTY_STOCK,
	c.REMARK,
	c.ORDER_DATE,
	c.DT_DUE,
	c.DT_EXPIRED,
	CONCAT(d.DESCRIPTION, CONCAT(e.OUTLET_NAME, f.SUPPLIER_NAME)) AS name_cd,
	g.OUTLET_NAME,
	g.ADDRESS_1,
	g.ADDRESS_2,
	g.PHONE,
	(SELECT DESCRIPTION FROM M_GLOBAL mg WHERE COND = 'ISO_RPT' AND CODE = 'RO') AS ISO_RPT_DESC
FROM
	T_ORDER_DETAIL a
LEFT JOIN M_ITEM b ON
	a.ITEM_CODE = b.ITEM_CODE
LEFT JOIN T_ORDER_HEADER c ON
	a.ORDER_NO = c.ORDER_NO
LEFT JOIN M_GLOBAL d ON
	c.CD_SUPPLIER = d.CODE
	AND d.COND = 'X_BGR'
LEFT JOIN M_OUTLET e ON
	c.CD_SUPPLIER = e.OUTLET_CODE
LEFT JOIN M_SUPPLIER f ON
	c.CD_SUPPLIER = f.CD_SUPPLIER
LEFT JOIN M_OUTLET g ON
	a.OUTLET_CODE = g.OUTLET_CODE
WHERE
	a.ORDER_NO = $P{orderNo}
	AND
	a.OUTLET_CODE = $P{outletCode}]]>
	</queryString>
