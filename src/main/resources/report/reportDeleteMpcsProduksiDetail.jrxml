	<queryString>
		<![CDATA[select TO_CHAR(A.MPCS_DATE, 'DD/MM/YYYY') AS MPCS_DATE, A.MPCS_GROUP,B.RECIPE_REMARK,A.SEQ_MPCS,
A.USER_UPD,C.STAFF_NAME ,A.DATE_UPD, TO_CHAR(TO_DATE(A.TIME_UPD, 'HH24MISS'), 'HH24:MI:SS') AS TIME_UPD, C.STAFF_NAME, count(*) AS SUB_TOTAL,
  sum(A.QUANTITY) AS QUANTITY, D.OUTLET_NAME, D.OUTLET_CODE, D.ADDRESS_1, D.ADDRESS_2, D.PHONE from t_mpcs_hist a
join m_recipe_header b
  on A.MPCS_GROUP=B.MPCS_GROUP
  and A.MPCS_DATE between $P{fromDate} and $P{toDate}
  and fryer_type='D'
join m_staff c
  on A.USER_UPD=C.STAFF_CODE
JOIN M_OUTLET d
  ON A.OUTLET_CODE = D.OUTLET_CODE
  GROUP BY A.MPCS_DATE, A.MPCS_GROUP, B.RECIPE_REMARK, A.SEQ_MPCS, A.USER_UPD, A.DATE_UPD,A.TIME_UPD, C.STAFF_NAME, D.OUTLET_NAME, D.OUTLET_CODE, D.ADDRESS_1, D.ADDRESS_2, D.PHONE
order by A.MPCS_DATE,A.SEQ_MPCS,A.TIME_UPD]]>
	</queryString>
