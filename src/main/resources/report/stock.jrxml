    <queryString>
        <![CDATA[SELECT a.TRANS_DATE, a.ITEM_CODE, b.ITEM_DESCRIPTION, a.QTY_BEGINNING, a.QTY_IN, a.QTY_OUT, (a.QTY_BEGINNING + a.QTY_IN - a.QTY_OUT) AS qty_ending, b.UOM_STOCK,
d.OUTLET_NAME, d."TYPE", d.ADDRESS_1, d.ADDRESS_2
FROM T_STOCK_CARD a
LEFT JOIN M_ITEM  b ON a.ITEM_CODE = b.ITEM_CODE
LEFT JOIN M_GLOBAL c ON b.CD_WAREHOUSE = c.CODE  AND c.COND = 'WAREHOUSE'
JOIN M_OUTLET d ON d.OUTLET_CODE = a.OUTLET_CODE
WHERE a.OUTLET_CODE = $P{outletCode} AND a.TRANS_DATE BETWEEN $P{fromDate} AND $P{toDate} $P!{query} ORDER BY a.TRANS_DATE, a.ITEM_CODE ASC]]>
    </queryString>
