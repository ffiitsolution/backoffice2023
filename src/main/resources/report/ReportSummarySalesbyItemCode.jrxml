		<queryString>
			<![CDATA[    SELECT
      SUM (TOTAL_AMOUNT) AS total_amount,
      SUM (total_discount) AS TOTAL_DISCOUNT,
      SUM (TOTAL_AMOUNT - TOTAL_DISCOUNT) AS sumamt
    FROM
      T_POS_BILL
    WHERE
      outlet_code = $P{outletCode}
      AND trans_date BETWEEN $P{fromDate}
      AND $P{toDate}
      AND pos_code BETWEEN $P{pos1}
      AND $P{pos2}
      AND shift_code BETWEEN $P{shift1}
      AND $P{shift2}
      AND cashier_code BETWEEN $P{cashier1}
      AND $P{cashier2}
      AND bill_time BETWEEN $P{fromTime}
      AND $P{toTime}
      and delivery_status = 'CLS']]>
		</queryString>
