	<queryString language="SQL">
		<![CDATA[SELECT mrh."RECIPE_CODE",
	mrh."RECIPE_REMARK",
	mrd."QTY_STOCK" AS RECIPE_ITEM_QTY,
	mrd."UOM_STOCK" AS RECIPE_UOM_STOCK,
	mi."ITEM_CODE" AS RECIPE_ITEM_CODE,
	mi."ITEM_DESCRIPTION" AS RECIPE_ITEM_NAME,
	mi2."ITEM_CODE" AS PRODUCT_ITEM_CODE,
	mi2."ITEM_DESCRIPTION" AS PRODUCT_ITEM_NAME,
	mrp."QTY_STOCK" AS PRDUCT_ITEM_STOCK_OUTPUT,
	mrp."UOM_STOCK" AS PRODUCT_ITEM_UOM_STOCK
FROM "M_RECIPE_DETAIL" mrd
	JOIN "M_RECIPE_HEADER" mrh ON 
	 mrh."RECIPE_CODE" = mrd."RECIPE_CODE" 
	JOIN "M_ITEM" mi ON 
	 mi."ITEM_CODE" = mrd."ITEM_CODE" 
	JOIN "M_RECIPE_PRODUCT" mrp ON 
	 mrp."RECIPE_CODE" = mrd."RECIPE_CODE" 
	JOIN "M_ITEM" mi2 ON 
	 mi2."ITEM_CODE" = mrp."PRODUCT_CODE" 
WHERE 
	 mrd."RECIPE_CODE" = $P{recipeCode}]]>
	</queryString>
