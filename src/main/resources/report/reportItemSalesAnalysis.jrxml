    <queryString>
        <![CDATA[SELECT
    a.GROUP_CODE,
    b.DESCRIPTION,
    a.ITEM_CODE,
    c.ITEM_DESCRIPTION,
    SUM(CASE WHEN TO_CHAR(a.TRANS_DATE, 'DAY') = 'SUNDAY   ' THEN a.QTY_EI + a.QTY_TA ELSE 0 END) AS MINGGU_QTY,
    SUM(CASE WHEN TO_CHAR(a.TRANS_DATE, 'DAY') = 'MONDAY   ' THEN a.QTY_EI + a.QTY_TA ELSE 0 END) AS SENIN_QTY,
    SUM(CASE WHEN TO_CHAR(a.TRANS_DATE, 'DAY') = 'TUESDAY  ' THEN a.QTY_EI + a.QTY_TA ELSE 0 END) AS SELASA_QTY,
    SUM(CASE WHEN TO_CHAR(a.TRANS_DATE, 'DAY') = 'WEDNESDAY' THEN a.QTY_EI + a.QTY_TA ELSE 0 END) AS RABU_QTY,
    SUM(CASE WHEN TO_CHAR(a.TRANS_DATE, 'DAY') = 'THURSDAY ' THEN a.QTY_EI + a.QTY_TA ELSE 0 END) AS KAMIS_QTY,
    SUM(CASE WHEN TO_CHAR(a.TRANS_DATE, 'DAY') = 'FRIDAY   ' THEN a.QTY_EI + a.QTY_TA ELSE 0 END) AS JUMAT_QTY,
    SUM(CASE WHEN TO_CHAR(a.TRANS_DATE, 'DAY') = 'SATURDAY ' THEN a.QTY_EI + a.QTY_TA ELSE 0 END) AS SABTU_QTY,
    SUM(CASE WHEN TO_CHAR(a.TRANS_DATE, 'DAY') = 'SUNDAY   ' THEN a.AMT_EI  + a.AMT_TA ELSE 0 END) AS MINGGU_AMT,
    SUM(CASE WHEN TO_CHAR(a.TRANS_DATE, 'DAY') = 'MONDAY   ' THEN a.AMT_EI  + a.AMT_TA ELSE 0 END) AS SENIN_AMT,
    SUM(CASE WHEN TO_CHAR(a.TRANS_DATE, 'DAY') = 'TUESDAY  ' THEN a.AMT_EI  + a.AMT_TA ELSE 0 END) AS SELASA_AMT,
    SUM(CASE WHEN TO_CHAR(a.TRANS_DATE, 'DAY') = 'WEDNESDAY' THEN a.AMT_EI  + a.AMT_TA ELSE 0 END) AS RABU_AMT,
    SUM(CASE WHEN TO_CHAR(a.TRANS_DATE, 'DAY') = 'THURSDAY ' THEN a.AMT_EI  + a.AMT_TA ELSE 0 END) AS KAMIS_AMT,
    SUM(CASE WHEN TO_CHAR(a.TRANS_DATE, 'DAY') = 'FRIDAY   ' THEN a.AMT_EI  + a.AMT_TA ELSE 0 END) AS JUMAT_AMT,
    SUM(CASE WHEN TO_CHAR(a.TRANS_DATE, 'DAY') = 'SATURDAY ' THEN a.AMT_EI  + a.AMT_TA ELSE 0 END) AS SABTU_AMT,
    TO_CHAR(NEXT_DAY(TRUNC(TO_DATE($P{date}, 'DD-Mon-YY')-7, 'IW'),'SUNDAY'),'DD Mon YYYY') AS from_date,
    TO_CHAR(NEXT_DAY(TRUNC(TO_DATE($P{date}, 'DD-Mon-YY'), 'IW'),'SATURDAY'),'DD Mon YYYY') AS to_date,
    d.OUTLET_NAME,
    e.DESCRIPTION AS type_outlet
FROM
    TMP_SALES_BY_ITEM a
LEFT JOIN
	M_GLOBAL b ON
	a.GROUP_CODE = b.CODE AND b.COND = 'GROUP'
LEFT JOIN
	M_ITEM c ON
	a.ITEM_CODE = c.ITEM_CODE
LEFT JOIN
	M_OUTLET d ON
	a.OUTLET_CODE = d.OUTLET_CODE
LEFT JOIN
	M_GLOBAL e ON
	d."TYPE" = e.CODE AND e.COND = 'OUTLET_TP'
WHERE
    a.OUTLET_CODE = $P{outletCode}
    AND a.TRANS_DATE BETWEEN TO_CHAR(NEXT_DAY(TRUNC(TO_DATE($P{date}, 'DD-Mon-YY')-7, 'IW'),'SUNDAY'),'DD-Mon-YY') AND TO_CHAR(NEXT_DAY(TRUNC(TO_DATE($P{date}, 'DD-Mon-YY'), 'IW'),'SATURDAY'),'DD-Mon-YY')
    AND a.POS_CODE BETWEEN $P{posCode1} AND $P{posCode2}
    AND a.CASHIER_CODE BETWEEN $P{cashierCode1} AND $P{cashierCode2}
    AND a.SHIFT_CODE BETWEEN $P{shiftCode1} AND $P{shiftCode2}
GROUP BY
    a.GROUP_CODE,
    a.ITEM_CODE,
    b.DESCRIPTION,
    c.ITEM_DESCRIPTION,
    d.OUTLET_NAME,
    e.DESCRIPTION
ORDER BY
    a.GROUP_CODE,
    a.ITEM_CODE ASC]]>
