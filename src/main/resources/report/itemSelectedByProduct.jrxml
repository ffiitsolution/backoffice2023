	<queryString>
		<![CDATA[SELECT
    mi.ITEM_DESCRIPTION AS DESCRIPTION,
    S.*,
    CASE WHEN $P{fromDate} = $P{toDate} THEN $P{fromDate} ELSE $P{fromDate} || ' - ' || $P{toDate} END AS PERIODE,
    mo.OUTLET_CODE,
    mo.OUTLET_NAME,
    mo.ADDRESS_1,
    mo.ADDRESS_2,
    mo.PHONE,
    ms.STAFF_NAME AS PRINT_NAME,
    SUM(S.TOT_STOCK) OVER (PARTITION BY S.MENU_ITEM_CODE) AS GROUP_TOT_STOCK
FROM (
    SELECT
        T.ITEM_CODE,
        T.MENU_ITEM_CODE,
        T.ITEM_DESCRIPTION,
        T.MENU_CODE,
        T.MENU_DESCRIPTION,
        SUM(T.STOCK_TA + T.STOCK_EI) AS TOT_STOCK
    FROM (
        -- Query 1
        SELECT
            A.TRANS_DATE,
            A.MENU_ITEM_CODE,
            D.ITEM_CODE,
            D.ITEM_DESCRIPTION,
            E.ITEM_CODE AS MENU_CODE,
            E.ITEM_DESCRIPTION AS MENU_DESCRIPTION ,
            SUM(A.ITEM_QTY * D.QTY_STOCK_E) AS STOCK_EI,
            0 AS STOCK_TA,
            0 AS STOCK_DETAIL
        FROM
            T_POS_BILL_ITEM_DETAIL A
            JOIN T_POS_BILL B ON A.BILL_NO = B.BILL_NO
                AND A.TRANS_DATE = B.TRANS_DATE
                AND A.DAY_SEQ = B.DAY_SEQ
                AND A.POS_CODE = B.POS_CODE
                AND A.OUTLET_CODE = B.OUTLET_CODE
                AND B.DELIVERY_STATUS = 'CLS'
            JOIN T_POS_BILL_ITEM C ON A.BILL_NO = C.BILL_NO
                AND A.TRANS_DATE = C.TRANS_DATE
                AND A.DAY_SEQ = C.DAY_SEQ
                AND A.POS_CODE = C.POS_CODE
                AND A.OUTLET_CODE = C.OUTLET_CODE
                AND A.ITEM_SEQ = C.ITEM_SEQ
                AND C.TRANS_TYPE = 'EI'
            JOIN M_GROUP_ITEM D ON A.MENU_ITEM_CODE = D.GROUP_ITEM_CODE
                AND D.STATUS = 'A'
            JOIN M_ITEM E ON E.ITEM_CODE = C.MENU_ITEM_CODE
        WHERE
            A.TRANS_DATE BETWEEN $P{fromDate} AND $P{toDate}
            AND A.OUTLET_CODE = $P{outletCode}
            AND D.ITEM_CODE = $P{itemCode}
		GROUP BY
		    A.TRANS_DATE,
		    A.MENU_ITEM_CODE,
		    D.ITEM_CODE,
		    D.ITEM_DESCRIPTION,
		    E.ITEM_CODE,
		    E.ITEM_DESCRIPTION

        UNION ALL

        -- Query 2
        SELECT
            A.TRANS_DATE,
            A.MENU_ITEM_CODE,
            D.ITEM_CODE,
            D.ITEM_DESCRIPTION,
            E.ITEM_CODE AS MENU_CODE,
            E.ITEM_DESCRIPTION AS MENU_DESCRIPTION ,
            0 AS STOCK_EI,
            SUM(A.ITEM_QTY * D.QTY_STOCK_T) AS STOCK_TA,
            0 AS STOCK_DETAIL
        FROM
            T_POS_BILL_ITEM_DETAIL A
            JOIN T_POS_BILL B ON A.BILL_NO = B.BILL_NO
                AND A.TRANS_DATE = B.TRANS_DATE
                AND A.DAY_SEQ = B.DAY_SEQ
                AND A.POS_CODE = B.POS_CODE
                AND A.OUTLET_CODE = B.OUTLET_CODE
                AND B.DELIVERY_STATUS = 'CLS'
            JOIN T_POS_BILL_ITEM C ON A.BILL_NO = C.BILL_NO
                AND A.TRANS_DATE = C.TRANS_DATE
                AND A.DAY_SEQ = C.DAY_SEQ
                AND A.POS_CODE = C.POS_CODE
                AND A.OUTLET_CODE = C.OUTLET_CODE
                AND A.ITEM_SEQ = C.ITEM_SEQ
                AND (C.TRANS_TYPE = 'TA' OR C.TRANS_TYPE = ' ' OR C.TRANS_TYPE IS NULL)
            JOIN M_GROUP_ITEM D ON A.MENU_ITEM_CODE = D.GROUP_ITEM_CODE
                AND D.STATUS = 'A'
            JOIN M_ITEM E ON E.ITEM_CODE = C.MENU_ITEM_CODE
        WHERE
            A.TRANS_DATE BETWEEN $P{fromDate} AND $P{toDate}
            AND A.OUTLET_CODE = $P{outletCode}
            AND D.ITEM_CODE = $P{itemCode}
		GROUP BY
		    A.TRANS_DATE,
		    A.MENU_ITEM_CODE,
		    D.ITEM_CODE,
		    D.ITEM_DESCRIPTION,
		    E.ITEM_CODE,
		    E.ITEM_DESCRIPTION

        UNION ALL

        -- Query 3
        SELECT
            A.TRANS_DATE,
            A.MENU_ITEM_CODE,
            D.ITEM_CODE,
            D.ITEM_DESCRIPTION,
            E.ITEM_CODE AS MENU_CODE,
            E.ITEM_DESCRIPTION AS MENU_DESCRIPTION ,
            SUM(A.ITEM_QTY * D.QTY_STOCK_E) AS STOCK_EI,
            0 AS STOCK_TA,
            0 AS STOCK_DETAIL
        FROM
            T_POS_BILL_ITEM A
            JOIN T_POS_BILL B ON A.TRANS_DATE = B.TRANS_DATE
                AND A.POS_CODE = B.POS_CODE
                AND A.OUTLET_CODE = B.OUTLET_CODE
                AND A.DAY_SEQ = B.DAY_SEQ
                AND A.BILL_NO = B.BILL_NO
                AND B.DELIVERY_STATUS = 'CLS'
                AND A.TRANS_TYPE = 'EI'
            JOIN M_MENU_ITEM C ON A.MENU_ITEM_CODE = C.MENU_ITEM_CODE
                AND C.MENU_SET = 'Y'
            JOIN M_GROUP_ITEM D ON C.MENU_ITEM_CODE = D.GROUP_ITEM_CODE
                AND D.STATUS = 'A'
            JOIN M_ITEM E ON E.ITEM_CODE = C.MENU_ITEM_CODE
        WHERE
            A.TRANS_DATE BETWEEN $P{fromDate} AND $P{toDate}
            AND A.OUTLET_CODE = $P{outletCode}
            AND D.ITEM_CODE = $P{itemCode}
		GROUP BY
		    A.TRANS_DATE,
		    A.MENU_ITEM_CODE,
		    D.ITEM_CODE,
		    D.ITEM_DESCRIPTION,
		    E.ITEM_CODE,
		    E.ITEM_DESCRIPTION

        UNION ALL

        -- Query 4
        SELECT
            A.TRANS_DATE,
            A.MENU_ITEM_CODE,
            D.ITEM_CODE,
            D.ITEM_DESCRIPTION,
            E.ITEM_CODE AS MENU_CODE,
            E.ITEM_DESCRIPTION AS MENU_DESCRIPTION ,
            0 AS STOCK_EI,
            SUM(A.ITEM_QTY * D.QTY_STOCK_T) AS STOCK_TA,
            0 AS STOCK_DETAIL
        FROM
            T_POS_BILL_ITEM A
            JOIN T_POS_BILL B ON A.TRANS_DATE = B.TRANS_DATE
                AND A.POS_CODE = B.POS_CODE
                AND A.OUTLET_CODE = B.OUTLET_CODE
                AND A.DAY_SEQ = B.DAY_SEQ
                AND A.BILL_NO = B.BILL_NO
                AND B.DELIVERY_STATUS = 'CLS'
                AND (A.TRANS_TYPE = 'TA' OR A.TRANS_TYPE = ' ' OR A.TRANS_TYPE IS NULL)
            JOIN M_MENU_ITEM C ON A.MENU_ITEM_CODE = C.MENU_ITEM_CODE
                AND C.MENU_SET = 'Y'
            JOIN M_GROUP_ITEM D ON C.MENU_ITEM_CODE = D.GROUP_ITEM_CODE
                AND D.STATUS = 'A'
            JOIN M_ITEM E ON E.ITEM_CODE = C.MENU_ITEM_CODE
        WHERE
            A.TRANS_DATE BETWEEN $P{fromDate} AND $P{toDate}
            AND A.OUTLET_CODE = $P{outletCode}
            AND D.ITEM_CODE = $P{itemCode}
		GROUP BY
		    A.TRANS_DATE,
		    A.MENU_ITEM_CODE,
		    D.ITEM_CODE,
		    D.ITEM_DESCRIPTION,
		    E.ITEM_CODE,
		    E.ITEM_DESCRIPTION
    ) T
    GROUP BY
        T.ITEM_CODE,
        T.MENU_ITEM_CODE,
        T.ITEM_DESCRIPTION,
        T.MENU_CODE,
        T.MENU_DESCRIPTION
) S
LEFT JOIN M_ITEM mi ON S.ITEM_CODE = mi.ITEM_CODE
LEFT JOIN M_STAFF ms ON ms.STAFF_CODE = $P{userUpd}
LEFT JOIN M_OUTLET mo ON mo.OUTLET_CODE = $P{outletCode}
ORDER BY S.MENU_ITEM_CODE, S.MENU_CODE, S.TOT_STOCK]]>
	</queryString>
