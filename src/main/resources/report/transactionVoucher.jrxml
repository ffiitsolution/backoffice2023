	<queryString>
		<![CDATA[SELECT
	A.VOUCHER_REF_NO,
	A.PAYMENT_AMOUNT,
	A.TRANS_DATE,
	C.OUTLET_CODE,
	C.OUTLET_NAME AS OUTLET_CODE_NAME,
	C.ADDRESS_1 AS OUTLET_CODE_ADDR_1,
	C.ADDRESS_2 AS OUTLET_CODE_ADDR_2,
		C.PHONE AS OUTLET_CODE_PHONE
FROM
	T_POS_BILL_PAYMENT_DETAIL A
LEFT JOIN T_POS_BILL B ON
	B.BILL_NO = A.BILL_NO 
LEFT JOIN M_OUTLET C ON
	A.OUTLET_CODE = C.OUTLET_CODE
WHERE
	A.PAYMENT_METHOD_CODE = 'VCR'
	AND A.OUTLET_CODE = $P{outletCode}
	AND B.TRANS_DATE BETWEEN $P{fromDate} AND $P{toDate}
	AND B.POS_CODE BETWEEN $P{posCode1} AND $P{posCode2}
	AND B.CASHIER_CODE BETWEEN $P{cashierCode1} AND $P{cashierCode2}
	AND B.SHIFT_CODE BETWEEN $P{shiftCode1} AND $P{shiftCode2}
	AND (B.DELIVERY_STATUS IN (' ', 'CLS')
		OR B.DELIVERY_STATUS IS NULL)
	AND B.order_type 
        IN (
	SELECT
		CODE
	FROM
		M_GLOBAL
	WHERE
		DESCRIPTION = 'GRPTP'
		AND COND BETWEEN 
         $P{orderType1} AND $P{orderType2})
ORDER BY
	TRANS_DATE ASC]]>
	</queryString>
