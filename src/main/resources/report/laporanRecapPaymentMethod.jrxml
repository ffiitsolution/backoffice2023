	<queryString>
		<![CDATA[SELECT
	DATA.*,
	otl.OUTLET_NAME AS OUTLET_CODE_NAME,
	otl.ADDRESS_1 AS OUTLET_CODE_ADDR_1,
	otl.ADDRESS_2 AS OUTLET_CODE_ADDR_2,
		otl.PHONE AS OUTLET_CODE_PHONE
FROM
	(
	SELECT
		a.OUTLET_CODE,
		A.TRANS_DATE AS TRANS_DATE,
		A.ORDER_TYPE,
		C.DESCRIPTION AS ORDER_TYPE_DESC,
		B.PAYMENT_METHOD_CODE,
		D.DESCRIPTION AS PAYMENT_METHOD_CODE_DESC,
		COUNT(*) AS COUNT_PAYMENT,
		SUM(B.PAYMENT_USED) AS TOTAL_PAYMENT
	FROM
		T_POS_BILL A
	JOIN T_POS_BILL_PAYMENT B ON
		A.BILL_NO = B.BILL_NO
	LEFT JOIN M_GLOBAL C ON
		C.COND = 'ORDER_TYPE'
		AND A.ORDER_TYPE = C.CODE
	LEFT JOIN M_GLOBAL D ON
		D.COND = 'PAY_CARD'
		AND B.PAYMENT_METHOD_CODE = D.CODE
	WHERE
		A.TRANS_DATE BETWEEN $P{fromDate} AND $P{toDate}
		AND A.OUTLET_CODE = $P{outletCode}
		AND A.POS_CODE BETWEEN $P{posCode1} AND $P{posCode2}
        AND A.CASHIER_CODE BETWEEN $P{cashierCode1} AND $P{cashierCode2} AND A.SHIFT_CODE BETWEEN $P{shiftCode1} AND $P{shiftCode2}
        AND (A.DELIVERY_STATUS IN (' ','CLS') OR A.DELIVERY_STATUS IS NULL) and A.order_type 
        IN (SELECT CODE FROM M_GLOBAL WHERE DESCRIPTION = 'GRPTP' AND COND BETWEEN  
         $P{orderType1} AND $P{orderType2}) 
	GROUP BY
		a.OUTLET_CODE,
		A.TRANS_DATE,
		A.ORDER_TYPE,
		C.DESCRIPTION,
		B.PAYMENT_METHOD_CODE,
		D.DESCRIPTION) DATA
LEFT JOIN M_OUTLET otl ON
	DATA.OUTLET_CODE = otl.outlet_code ORDER BY DATA.TRANS_DATE ASC, DATA.PAYMENT_METHOD_CODE ASC]]>
	</queryString>
