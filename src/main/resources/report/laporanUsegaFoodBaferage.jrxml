	<queryString>
		<![CDATA[select a.item_code, c.item_description, COALESCE(SALES,0) SALES, COALESCE(MEAL,0) MEAL, COALESCE(PRODUKSI, 0) PRODUKSI, COALESCE(WASTAGE, 0) WASTAGE, COALESCE(LEFT_OVER, 0 ) LEFT_OVER, COALESCE(total_usage, 0) TOTAL_USAGE,COALESCE(b.item_cost, 0) ITEM_COST,(total_usage*COALESCE(b.item_cost, 0)) as total_cost,
	COALESCE(C.CONV_STOCK, 0) CONV_STOCK,
	COALESCE(C.UOM_STOCK, '' ) UOM_STOCK, D.OUTLET_CODE,
	D.OUTLET_NAME AS OUTLET_CODE_NAME,
	D.ADDRESS_1 AS OUTLET_CODE_ADDR_1,
	D.ADDRESS_2 AS OUTLET_CODE_ADDR_2,
	D.PHONE AS OUTLET_CODE_PHONE from (
select item_code,SUM(SALES) as SALES,SUM(FREE_MEAL) as MEAL,SUM(PRODUKSI) AS PRODUKSI,SUM(WASTAGE) as WASTAGE,SUM(Left_Over)AS LEFT_OVER,(SUM(SALES)+SUM(FREE_MEAL)+SUM(PRODUKSI)+SUM(WASTAGE)+SUM(Left_Over))as TOTAL_USAGE
from (
select item_code,case when cd_trans='SLS' then total else 0 end as SALES, case when cd_trans='FML' then total else 0 end as FREE_MEAL, case when cd_trans='PRD' then total else 0 end as PRODUKSI,
case when cd_trans='WST' then total else 0 end as WASTAGE,case when cd_trans='LOV' then total else 0 end as Left_Over 
from (
select item_code, case when cd_Trans = 'DEL'then 'PRD'else cd_Trans end as 
cd_Trans, sum(quantity_in),sum(quantity),abs(sum(quantity_in)-sum(quantity)) as total from t_stock_card_detail 
where outlet_code=$P{outletCode} and trans_date between $P{fromDate} and $P{toDate} 
group by item_code,case when cd_Trans = 'DEL'then 'PRD'else cd_Trans end 
order by item_code,case when cd_Trans = 'DEL'then 'PRD'else cd_Trans end
)
) group by item_code) a left join m_item_cost b on a.item_code=b.item_code 
LEFT JOIN m_item c ON a.item_code = c.item_code 
LEFT JOIN M_OUTLET D ON D.outlet_code = $P{outletCode}
WHERE 
 	c.cd_level_4 <> 'Y' AND c.status <> 'I' AND $P!{query1}	
order by item_code]]>
	</queryString>
