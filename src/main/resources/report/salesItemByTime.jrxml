	<queryString>
		<![CDATA[SELECT
	max(o.OUTLET_NAME) AS OUTLET_NAME,
	max(o.ADDRESS_1) AS ADDRESS_1,
	max(o.ADDRESS_2) AS ADDRESS_2,
	max(o.PHONE) AS PHONE,
    a.menu_item_code as item_code,
    mi.ITEM_DESCRIPTION,
    SUM(CASE WHEN a.trans_type = 'EI' THEN a.item_qty ELSE 0 END) AS QTY_EI,
    SUM(CASE 
	    WHEN a.trans_type = ' ' THEN a.item_qty 
	    WHEN a.trans_type = 'TA' THEN a.item_qty 
	    ELSE 0 
    END) AS QTY_TA
FROM t_pos_bill_item a
JOIN t_pos_bill b 
	ON a.outlet_code = b.outlet_code AND a.pos_code = b.pos_code AND a.bill_no = b.bill_no
JOIN M_ITEM mi 
	ON a.menu_item_code = mi.item_code
JOIN M_OUTLET o
	ON a.OUTLET_CODE = o.OUTLET_CODE
WHERE
    b.outlet_code = '0321'
    AND b.trans_date BETWEEN $P{fromDate} AND $P{toDate}
    AND b.pos_code BETWEEN $P{posCode1} AND $P{posCode2}
    AND b.shift_code BETWEEN $P{shiftCode1} AND $P{shiftCode2}
    AND b.cashier_code BETWEEN $P{cashierCode1} AND $P{cashierCode2}
    AND b.bill_time BETWEEN $P{fromTime} AND $P{toTime}
    AND (b.delivery_status IN (' ', 'CLS') OR b.delivery_status IS NULL)
GROUP BY a.menu_item_code, mi.ITEM_DESCRIPTION
ORDER BY a.menu_item_code]]>
	</queryString>
