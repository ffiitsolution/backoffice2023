	<queryString>
		<![CDATA[SELECT
    b.menu_item_code AS Code,
    c.item_description,
    a.menu_item_code AS Codes,
    d.item_description AS PLU_Name,
    e.outlet_name,
    e.address_1 as first_address,
    e.address_2 as second_address,
    SUM(b.item_qty)
FROM
    t_pos_bill_item a
JOIN
    t_pos_bill_item_detail b
    ON a.bill_no = b.bill_no
    AND a.pos_code = b.pos_code
    AND a.trans_date = b.trans_date
    AND a.outlet_code = b.outlet_code
    AND a.region_code = b.region_code
    AND a.day_seq = b.day_seq
    AND a.item_seq = b.item_seq
    AND b.menu_item_code IN ($P!{menuItemCodes})
    AND b.trans_Date BETWEEN $P{fromDate} AND $P{toDate}
    AND b.outlet_code = $P{outletCode}
JOIN
    t_pos_bill c
    ON a.bill_no = c.bill_no
    AND a.pos_code = c.pos_code
    AND a.trans_date = c.trans_date
    AND a.outlet_code = c.outlet_code
    AND a.region_code = c.region_code
    AND a.day_seq = c.day_seq
    AND c.delivery_status = 'CLS'
    AND c.trans_Date BETWEEN $P{fromDate} AND $P{toDate}
    AND c.outlet_code = $P{outletCode}
JOIN
    m_item c
    ON b.menu_item_code = c.item_code
JOIN
    m_item d
    ON a.menu_item_code = d.item_code
JOIN
    m_outlet e ON a.outlet_code = e.outlet_code
GROUP BY
    b.menu_item_code, c.item_description, a.menu_item_code, d.item_description, b.menu_item_code, c.item_description, a.menu_item_code, d.item_description, e.outlet_name, e.address_1, e.address_2
ORDER BY
    b.menu_item_code, a.menu_item_code]]>
	</queryString>
