	<queryString>
		<![CDATA[SELECT
	NVL(s.STAFF_NAME, '-') AS CASHIER,
	p.POS_DESCRIPTION,
	t.*,
    CASE WHEN $P{fromDate} = $P{toDate} THEN $P{fromDate} ELSE $P{fromDate} || ' - ' || $P{toDate} END AS PERIODE,
    mo.OUTLET_CODE,
    mo.OUTLET_NAME,
    mo.ADDRESS_1,
    mo.ADDRESS_2,
    mo.PHONE,
    ms.STAFF_NAME AS PRINT_NAME
FROM T_POS_ACTION t 
LEFT JOIN M_POS_STAFF s ON s.STAFF_POS_CODE = t.CASHIER_CODE 
LEFT JOIN M_POS p ON p.POS_CODE = t.POS_CODE AND p.OUTLET_CODE = t.OUTLET_CODE 
LEFT JOIN M_STAFF ms ON ms.STAFF_CODE = nvl($P{userUpd},'SYSTEM')
LEFT JOIN M_OUTLET mo ON mo.OUTLET_CODE = t.OUTLET_CODE
WHERE
    t.OUTLET_CODE = $P{outletCode} AND
    TRUNC(t.DATE_UPD) BETWEEN TO_DATE($P{fromDate}, 'DD MON RR') AND TO_DATE($P{toDate}, 'DD MON RR')
	AND t.pos_code BETWEEN $P{posCode1} AND $P{posCode2}
	AND t.cashier_code BETWEEN $P{cashierCode1} AND $P{cashierCode2} 
    AND ($P{status} = 'ALL' OR t.ORDER_STATUS = $P{status})
ORDER BY t.BILL_NO, t.DATE_UPD]]>
	</queryString>
