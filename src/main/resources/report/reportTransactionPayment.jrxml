	<queryString>
		<![CDATA[SELECT 
	G.*,
    COALESCE(ms.STAFF_NAME, ms.STAFF_FULL_NAME, $P{user}) AS PRINT_NAME
FROM (
SELECT
    A.OUTLET_CODE,
    A.TRANS_DATE,
    TO_DATE(A.BILL_TIME, 'hh24:mi:ss') AS BILL_TIME,
    A.POS_CODE,
    B.STAFF_NAME,
    A.SHIFT_CODE,
    A.BILL_NO,
    A.PAYMENT_AMOUNT,
    A.PAYMENT_USED,
    C.OUTLET_NAME,
    C.ADDRESS_1,
    C.ADDRESS_2,
    C.CITY,
    C.TYPE,
    C.PHONE,
    D.DESCRIPTION AS PAYMENT_TYPE,
    E.DESCRIPTION AS PAYMENT_METHOD,
    F.PAYMENT_USED_TOTAL
FROM (
    SELECT
        B.OUTLET_CODE,
        B.TRANS_DATE,
        A.PAYMENT_TYPE_CODE,
        A.PAYMENT_METHOD_CODE,
        B.POS_CODE,
        B.SHIFT_CODE,
        B.CASHIER_CODE,
        B.BILL_NO,
        B.BILL_TIME,
        C.PAYMENT_AMOUNT,
        C.PAYMENT_USED
    FROM
        M_PAYMENT_METHOD A,
        T_POS_BILL B,
        T_POS_BILL_PAYMENT C
    WHERE
        B.OUTLET_CODE = $P{outletCode}
        AND B.TRANS_DATE BETWEEN $P{transDate1} AND $P{transDate2}
        AND B.POS_CODE BETWEEN $P{posCode1} AND $P{posCode2}
        AND B.CASHIER_CODE BETWEEN $P{cashierCode1} AND $P{cashierCode2}
        AND B.BILL_TIME BETWEEN $P{billTime1} AND $P{billTime2}
        AND A.PAYMENT_TYPE_CODE BETWEEN $P{paymentType1} AND $P{paymentType2}
        AND A.PAYMENT_METHOD_CODE BETWEEN $P{paymentMethod1} AND $P{paymentMethod2}
        AND B.SHIFT_CODE BETWEEN $P{shiftCode1} AND $P{shiftCode2}
        AND A.OUTLET_CODE = B.OUTLET_CODE
        AND B.OUTLET_CODE = C.OUTLET_CODE
        AND B.TRANS_DATE = C.TRANS_DATE
        AND A.PAYMENT_METHOD_CODE = C.PAYMENT_METHOD_CODE
        AND B.POS_CODE = C.POS_CODE
        AND B.BILL_NO = C.BILL_NO
) A
LEFT JOIN (
    SELECT
        OUTLET_CODE,
        STAFF_POS_CODE,
        STAFF_NAME
    FROM
        M_POS_STAFF
    WHERE
        STATUS = 'A'
        AND ACCESS_LEVEL = 'KSR'
) B ON A.OUTLET_CODE = B.OUTLET_CODE AND A.CASHIER_CODE = B.STAFF_POS_CODE
LEFT JOIN (
    SELECT
        OUTLET_CODE,
        OUTLET_NAME,
        ADDRESS_1,
        ADDRESS_2,
        PHONE,
        CITY,
        TYPE
    FROM
        M_OUTLET
) C ON A.OUTLET_CODE = C.OUTLET_CODE
LEFT JOIN (
    SELECT
        COND,
        CODE,
        DESCRIPTION
    FROM
        M_GLOBAL
    WHERE
        COND LIKE '%PAY_TYPE%'
) D ON A.PAYMENT_TYPE_CODE = D.CODE
LEFT JOIN (
    SELECT
        COND,
        CODE,
        DESCRIPTION
    FROM
        M_GLOBAL
    WHERE
        COND LIKE '%PAY_METHOD%'
) E ON A.PAYMENT_METHOD_CODE = E.CODE
LEFT JOIN (
    SELECT
        OUTLET_CODE,
        PAYMENT_TYPE_CODE,
        SUM(PAYMENT_USED) AS PAYMENT_USED_TOTAL
    FROM (
        SELECT
            B.OUTLET_CODE,
            B.TRANS_DATE,
            A.PAYMENT_TYPE_CODE,
            C.PAYMENT_USED
        FROM
            M_PAYMENT_METHOD A,
            T_POS_BILL B,
            T_POS_BILL_PAYMENT C
        WHERE
            B.OUTLET_CODE = $P{outletCode}
            AND B.TRANS_DATE BETWEEN $P{transDate1} AND $P{transDate2}
            AND B.POS_CODE BETWEEN $P{posCode1} AND $P{posCode2}
            AND B.CASHIER_CODE BETWEEN $P{cashierCode1} AND $P{cashierCode2}
            AND B.BILL_TIME BETWEEN $P{billTime1} AND $P{billTime2}
            AND A.PAYMENT_TYPE_CODE BETWEEN $P{paymentType1} AND $P{paymentType2}
            AND A.PAYMENT_METHOD_CODE BETWEEN $P{paymentMethod1} AND $P{paymentMethod2}
            AND B.SHIFT_CODE BETWEEN $P{shiftCode1} AND $P{shiftCode2}
            AND A.OUTLET_CODE = B.OUTLET_CODE
            AND B.OUTLET_CODE = C.OUTLET_CODE
            AND B.TRANS_DATE = C.TRANS_DATE
            AND A.PAYMENT_METHOD_CODE = C.PAYMENT_METHOD_CODE
            AND B.POS_CODE = C.POS_CODE
            AND B.BILL_NO = C.BILL_NO
    )
    GROUP BY
        OUTLET_CODE,
        PAYMENT_TYPE_CODE
) F ON A.PAYMENT_TYPE_CODE = F.PAYMENT_TYPE_CODE
ORDER BY
    A.TRANS_DATE,
    A.PAYMENT_TYPE_CODE,
    A.PAYMENT_METHOD_CODE,
    A.POS_CODE,
    A.SHIFT_CODE,
    A.BILL_NO,
    A.BILL_TIME
   ) G 
LEFT JOIN M_STAFF ms ON $P{user} = ms.STAFF_CODE]]>
	</queryString>
