    <queryString>
        <![CDATA[SELECT a.ITEM_CODE, a.ITEM_DESCRIPTION, a.CONV_WAREHOUSE, a.UOM_PURCHASE, a.UOM_WAREHOUSE, a.CONV_STOCK, a.UOM_STOCK,
a.STATUS, CASE WHEN a.FLAG_STOCK = 'Y' THEN 'Stock' ELSE 'Non Stock' END type_stock, b.DESCRIPTION
FROM M_ITEM a
LEFT JOIN M_GLOBAL b ON a.CD_WAREHOUSE = b.CODE AND b.COND ='WAREHOUSE'
WHERE a.STATUS IN ($P{status1}, $P{status2}) AND a.FLAG_STOCK IN ($P{falgStock1}, $P{falgStock2}, $P{falgStock3})
ORDER BY ITEM_CODE ASC]]>
