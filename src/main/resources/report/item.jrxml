    <queryString>
        <![CDATA[SELECT a.ITEM_CODE, a.ITEM_DESCRIPTION, a.CONV_WAREHOUSE, a.UOM_PURCHASE, a.UOM_WAREHOUSE, a.CONV_STOCK, a.UOM_STOCK,
a.STATUS, CASE WHEN a.FLAG_STOCK = 'Y' THEN 'Stock' ELSE 'Non Stock' END type_stock, b.DESCRIPTION, c.OUTLET_NAME
FROM M_ITEM a
LEFT JOIN M_GLOBAL b ON a.CD_WAREHOUSE = b.CODE AND b.COND ='WAREHOUSE'
JOIN M_OUTLET c ON c.OUTLET_CODE = $P{outletCode}
WHERE a.STATUS IN ($P{status1}, $P{status2}) AND a.FLAG_STOCK IN ($P{flagStock1}, $P{flagStock2}, $P{flagStock3}) $P!{query}
ORDER BY ITEM_CODE ASC]]>
    </queryString>
