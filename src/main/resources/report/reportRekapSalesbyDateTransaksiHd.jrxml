    <queryString>
        <![CDATA[SELECT
	TRANS_DATE,
	STATUSES,
	SUM(TOTAL_TRX) AS TRX,
	SUM(AMT) AS AMT,
	CASE
		WHEN
		statuses = 'CLOSED' THEN SUM(TAX)
		ELSE 0
	END AS TAX,
	CASE
		WHEN STATUSES = 'CLOSED'
THEN SUM(BIAYA_KIRIM)
		ELSE 0
	END AS BIAYA_KIRIM,
	CASE
		WHEN statuses = 'CLOSED'
THEN SUM(TOTAL_SALES)
		ELSE 0
	END AS SALES,
	SUM(TUNAI) AS TUNAI,
	SUM(VOUCHER) AS VOUCHER,
	SUM(KARTU) AS KARTU,
	TIPE
FROM
	(
	SELECT
		TRANS_DATE,
		STATUSES,
		TOTAL_TRX,
		AMT,
		TAX,
		BIAYA_KIRIM,
		TOTAL_SALES,
		TUNAI,
		VOUCHER,
		KARTU,
		TIPE
	FROM
		(
		SELECT
			A.TRANS_DATE,
			CASE
				WHEN DELIVERY_STATUS = 'BAD' THEN 'BAD ORDER'
				WHEN DELIVERY_STATUS = 'CAN' THEN 'CANCEL'
				WHEN DELIVERY_STATUS = 'ORD' THEN 'ORDER'
				WHEN DELIVERY_STATUS = 'PCK' THEN 'PACK'
				WHEN DELIVERY_STATUS = 'DLV' THEN 'DELIVERED'
				WHEN DELIVERY_STATUS = 'CLS' THEN 'CLOSED'
				ELSE 'ANEH'
			END AS STATUSES,
			CASE
				WHEN A.BILL_NO LIKE '%W%' THEN 'W'
				WHEN A.BILL_NO LIKE '%M%' THEN 'M'
				WHEN A.BILL_NO NOT LIKE '%W%'
				AND A.BILL_NO NOT LIKE '%M%'
				AND A.FLAG_ONLINE =
'Y' THEN 'C'
				ELSE 'D'
			END AS TIPE,
			COUNT(A.BILL_NO) AS TOTAL_TRX,
			SUM(TOTAL_AMOUNT) AS AMT,
			SUM(TOTAL_TAX) AS TAX,
			SUM(TOTAL_CHARGE + TOTAL_TAX_CHARGE) AS BIAYA_KIRIM,
			SUM(TOTAL_SALES) AS
TOTAL_SALES,
			0 AS TUNAI,
			0 AS VOUCHER,
			0 AS KARTU,
			' ' AS PAYMENT_METHOD_CODE
		FROM
			T_POS_BILL A
		WHERE
			A.DELIVERY_STATUS BETWEEN $P{statusTrans1} AND $P{statusTrans2}
			AND A.OUTLET_CODE = $P{outletCode}
			AND
A.ORDER_TYPE = 'HMD'
			AND A.TRANS_dATE BETWEEN $P{fromDate} AND $P{toDate}
		GROUP BY
			A.TRANS_DATE,
			CASE
				WHEN DELIVERY_STATUS = 'BAD' THEN 'BAD ORDER'
				WHEN DELIVERY_STATUS = 'CAN' THEN 'CANCEL'
				WHEN DELIVERY_STATUS = 'ORD' THEN 'ORDER'
				WHEN DELIVERY_STATUS = 'PCK' THEN 'PACK'
				WHEN DELIVERY_STATUS = 'DLV' THEN 'DELIVERED'
				WHEN DELIVERY_STATUS = 'CLS' THEN 'CLOSED'
				ELSE 'ANEH'
			END,
			CASE
				WHEN A.BILL_NO LIKE '%W%' THEN 'W'
				WHEN A.BILL_NO LIKE '%M%' THEN 'M'
				WHEN A.BILL_NO NOT LIKE '%W%'
				AND A.BILL_NO NOT LIKE '%M%'
				AND A.FLAG_ONLINE =
'Y' THEN 'C'
				ELSE 'D'
			END
)
UNION ALL
	SELECT
		TRANS_DATE,
		STATUSES,
		TOTAL_TRX,
		AMT,
		TAX,
		BIAYA_KIRIM,
		TOTAL_SALES,
		TUNAI,
		VOUCHER,
		KARTU,
		TIPE
	FROM
		(
		SELECT
			A.TRANS_DATE,
			CASE
				WHEN DELIVERY_STATUS = 'BAD' THEN 'BAD ORDER'
				WHEN DELIVERY_STATUS = 'CAN' THEN 'CANCEL'
				WHEN DELIVERY_STATUS = 'ORD' THEN 'ORDER'
				WHEN DELIVERY_STATUS = 'PCK' THEN 'PACK'
				WHEN DELIVERY_STATUS = 'DLV' THEN 'DELIVERED'
				WHEN DELIVERY_STATUS = 'CLS' THEN 'CLOSED'
				ELSE 'ANEH'
			END AS STATUSES,
			CASE
				WHEN A.BILL_NO LIKE '%W%' THEN 'W'
				WHEN A.BILL_NO LIKE '%M%' THEN 'M'
				WHEN A.BILL_NO NOT LIKE '%W%'
					AND A.BILL_NO NOT LIKE '%M%'
					AND A.FLAG_ONLINE =
'Y' THEN 'C'
					ELSE 'D'
				END AS TIPE,
				0 AS TOTAL_TRX,
				0 AS AMT,
				0 AS TAX,
				0 AS BIAYA_KIRIM,
				0 AS TOTAL_SALES,
				SUM(PAYMENT_USED) AS TUNAI,
				0 AS VOUCHER,
				0
AS KARTU,
				B.PAYMENT_METHOD_CODE
			FROM
				T_POS_BILL A
			JOIN T_POS_BILL_PAYMENT B ON
				A.OUTLET_CODE = B.OUTLET_CODE
				AND A.POS_CODE =
B.POS_CODE
				AND
A.TRANS_dATE = B.TRANS_DATE
				AND A.BILL_NO = B.BILL_NO
				AND B.PAYMENT_METHOD_CODE
= 'CSH'
				AND A.ORDER_TYPE = 'HMD'
				WHERE A.DELIVERY_STATUS BETWEEN $P{statusTrans1} AND $P{statusTrans2}
				AND A.OUTLET_CODE = $P{outletCode}
				AND A.TRANS_dATE BETWEEN $P{fromDate} AND $P{toDate}
			GROUP BY
				A.TRANS_DATE,
				B.PAYMENT_METHOD_CODE,
				CASE
					WHEN DELIVERY_STATUS = 'BAD' THEN 'BAD ORDER'
					WHEN DELIVERY_STATUS = 'CAN' THEN 'CANCEL'
					WHEN DELIVERY_STATUS = 'ORD' THEN 'ORDER'
					WHEN DELIVERY_STATUS = 'PCK' THEN 'PACK'
					WHEN DELIVERY_STATUS = 'DLV' THEN 'DELIVERED'
					WHEN DELIVERY_STATUS = 'CLS' THEN 'CLOSED'
					ELSE 'ANEH'
				END,
				CASE
					WHEN A.BILL_NO LIKE '%W%' THEN 'W'
					WHEN A.BILL_NO LIKE '%M%' THEN 'M'
					WHEN A.BILL_NO NOT LIKE '%W%'
					AND A.BILL_NO NOT LIKE '%M%'
					AND A.FLAG_ONLINE =
'Y' THEN 'C'
					ELSE 'D'
				END
)
	WHERE
		PAYMENT_METHOD_CODE = 'CSH'
UNION ALL
	SELECT
		TRANS_DATE,
		STATUSES,
		TOTAL_TRX,
		AMT,
		TAX,
		BIAYA_KIRIM,
		TOTAL_SALES,
		TUNAI,
		VOUCHER,
		KARTU,
		TIPE
	FROM
		(
		SELECT
			A.TRANS_DATE,
			CASE
				WHEN DELIVERY_STATUS = 'BAD' THEN 'BAD ORDER'
				WHEN DELIVERY_STATUS = 'CAN' THEN 'CANCEL'
				WHEN DELIVERY_STATUS = 'ORD' THEN 'ORDER'
				WHEN DELIVERY_STATUS = 'PCK' THEN 'PACK'
				WHEN DELIVERY_STATUS = 'DLV' THEN 'DELIVERED'
				WHEN DELIVERY_STATUS = 'CLS' THEN 'CLOSED'
				ELSE 'ANEH'
			END AS STATUSES,
			CASE
				WHEN A.BILL_NO LIKE '%W%' THEN 'W'
				WHEN A.BILL_NO LIKE '%M%' THEN 'M'
				WHEN A.BILL_NO NOT LIKE '%W%'
					AND A.BILL_NO NOT LIKE '%M%'
					AND A.FLAG_ONLINE =
'Y' THEN 'C'
					ELSE 'D'
				END AS TIPE,
				0 AS TOTAL_TRX,
				0 AS AMT,
				0 AS TAX,
				0 AS BIAYA_KIRIM,
				0 AS TOTAL_SALES,
				0 AS TUNAI,
				0 AS VOUCHER,
				SUM(PAYMENT_USED)
AS KARTU,
				'CARD' AS PAYMENT_METHOD_CODE
			FROM
				T_POS_BILL A
			JOIN T_POS_BILL_PAYMENT B ON
				A.OUTLET_CODE = B.OUTLET_CODE
				AND A.POS_CODE =
B.POS_CODE
				AND
A.TRANS_dATE = B.TRANS_DATE
				AND A.BILL_NO = B.BILL_NO
				AND B.PAYMENT_METHOD_CODE
IN ('PNT', 'FLZ', 'MND', 'CRC')
					AND A.ORDER_TYPE = 'HMD'
					WHERE A.DELIVERY_STATUS BETWEEN $P{statusTrans1} AND $P{statusTrans2}
					AND A.OUTLET_CODE = $P{outletCode}
					AND A.TRANS_dATE BETWEEN $P{fromDate} AND $P{toDate}
				GROUP BY
					A.TRANS_DATE,
					CASE
						WHEN DELIVERY_STATUS = 'BAD' THEN 'BAD ORDER'
						WHEN DELIVERY_STATUS = 'CAN' THEN 'CANCEL'
						WHEN DELIVERY_STATUS = 'ORD' THEN 'ORDER'
						WHEN DELIVERY_STATUS = 'PCK' THEN 'PACK'
						WHEN DELIVERY_STATUS = 'DLV' THEN 'DELIVERED'
						WHEN DELIVERY_STATUS = 'CLS' THEN 'CLOSED'
						ELSE 'ANEH'
					END,
					CASE
						WHEN A.BILL_NO LIKE '%W%' THEN 'W'
						WHEN A.BILL_NO LIKE '%M%' THEN 'M'
						WHEN A.BILL_NO NOT LIKE '%W%'
						AND A.BILL_NO NOT LIKE '%M%'
						AND A.FLAG_ONLINE =
'Y' THEN 'C'
						ELSE 'D'
					END
)
	WHERE
		PAYMENT_METHOD_CODE = 'CARD'
UNION ALL
	SELECT
		TRANS_DATE,
		STATUSES,
		TOTAL_TRX,
		AMT,
		TAX,
		BIAYA_KIRIM,
		TOTAL_SALES,
		TUNAI,
		VOUCHER,
		KARTU,
		TIPE
	FROM
		(
		SELECT
			A.TRANS_DATE,
			CASE
				WHEN DELIVERY_STATUS = 'BAD' THEN 'BAD ORDER'
				WHEN DELIVERY_STATUS = 'CAN' THEN 'CANCEL'
				WHEN DELIVERY_STATUS = 'ORD' THEN 'ORDER'
				WHEN DELIVERY_STATUS = 'PCK' THEN 'PACK'
				WHEN DELIVERY_STATUS = 'DLV' THEN 'DELIVERED'
				WHEN DELIVERY_STATUS = 'CLS' THEN 'CLOSED'
				ELSE 'ANEH'
			END AS STATUSES,
			CASE
				WHEN A.BILL_NO LIKE '%W%' THEN 'W'
				WHEN A.BILL_NO LIKE '%M%' THEN 'M'
				WHEN A.BILL_NO NOT LIKE '%W%'
					AND A.BILL_NO NOT LIKE '%M%'
					AND A.FLAG_ONLINE =
'Y' THEN 'C'
					ELSE 'D'
				END AS TIPE,
				0 AS TOTAL_TRX,
				0 AS AMT,
				0 AS TAX,
				0 AS BIAYA_KIRIM,
				0 AS TOTAL_SALES,
				0 AS TUNAI,
				SUM(PAYMENT_USED) AS VOUCHER,
				0
AS KARTU,
				B.PAYMENT_METHOD_CODE
			FROM
				T_POS_BILL A
			JOIN T_POS_BILL_PAYMENT B ON
				A.OUTLET_CODE = B.OUTLET_CODE
				AND A.POS_CODE =
B.POS_CODE
				AND
A.TRANS_dATE = B.TRANS_DATE
				AND A.BILL_NO = B.BILL_NO
				AND B.PAYMENT_METHOD_CODE
= 'VCR'
				AND A.ORDER_TYPE = 'HMD'
				WHERE A.DELIVERY_STATUS BETWEEN $P{statusTrans1} AND $P{statusTrans2}
				AND A.OUTLET_CODE = $P{outletCode}
				AND A.TRANS_dATE BETWEEN $P{fromDate} AND $P{toDate}
			GROUP BY
				A.TRANS_DATE,
				B.PAYMENT_METHOD_CODE,
				CASE
					WHEN DELIVERY_STATUS = 'BAD' THEN 'BAD ORDER'
					WHEN DELIVERY_STATUS = 'CAN' THEN 'CANCEL'
					WHEN DELIVERY_STATUS = 'ORD' THEN 'ORDER'
					WHEN DELIVERY_STATUS = 'PCK' THEN 'PACK'
					WHEN DELIVERY_STATUS = 'DLV' THEN 'DELIVERED'
					WHEN DELIVERY_STATUS = 'CLS' THEN 'CLOSED'
					ELSE 'ANEH'
				END,
				CASE
					WHEN A.BILL_NO LIKE '%W%' THEN 'W'
					WHEN A.BILL_NO LIKE '%M%' THEN 'M'
					WHEN A.BILL_NO NOT LIKE '%W%'
					AND A.BILL_NO NOT LIKE '%M%'
					AND A.FLAG_ONLINE =
'Y' THEN 'C'
					ELSE 'D'
				END
)
	WHERE
		PAYMENT_METHOD_CODE = 'VCR'
)
WHERE
	TIPE BETWEEN $P{tipeTrans1} AND $P{tipeTrans2}
GROUP BY
