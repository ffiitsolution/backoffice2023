    <queryString>
        <![CDATA[select A.OUTLET_CODE,A.TRANS_TYPE as transtypeitem,Z.OUTLET_NAME,Z.ADDRESS_1 as outaddress1,Z.ADDRESS_2 as outaddress2,Z.PHONE,Z.CITY,Z.POST_CODE,a.menu_item_code,b.item_description,item_qty,
amount,total_amount,total_rounding,total_tax,total_sales,a.bill_no,c.pos_code,
c.cashier_code,c.shift_code,c.trans_type,c.order_type,c.delivery_status,
c.customer_phone_no,c.customer_name,c.address_1,c.city,c.zip_code, c.trans_date,D.PAYMENT_METHOD_CODE,E.DESCRIPTION AS PAYMENT_METHOD_DESCRIPTION from t_pos_bill_item a
left join m_item b
on a.menu_item_code=b.item_code
left join t_pos_bill c
on a.bill_no=c.bill_no
and a.trans_date=c.trans_date
LEFT JOIN M_OUTLET Z
ON A.OUTLET_CODE=Z.OUTLET_CODE
LEFT JOIN T_POS_BILL_PAYMENT D ON A.BILL_NO = D.BILL_NO AND A.TRANS_DATE = D.TRANS_DATE
LEFT JOIN M_GLOBAL E ON D.PAYMENT_METHOD_CODE = E.CODE AND E.COND = 'PAY_METHOD'
where a.bill_no=$P{billNo} AND A.TRANS_DATE=$P{transDate} AND A.OUTLET_CODE=$P{outletCode}]]>
    </queryString>
