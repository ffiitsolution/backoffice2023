	<queryString>
		<![CDATA[SELECT a.TRANS_DATE,
b.POS_TYPE ,
d.DESCRIPTION,
a.OUTLET_CODE,
c.OUTLET_NAME,
c.ADDRESS_1 ,
c.ADDRESS_2,
c.PHONE,
SUM(a.TOTAL_AMOUNT) AS PENJUALAN,
SUM(a.TOTAL_DISCOUNT) AS DISCOUNT,
SUM(a.TOTAL_CHARGE) AS PENDAPATAN_NON_TAX,
SUM(a.TOTAL_AMOUNT) AS PENDAPATAN,
SUM(a.TOTAL_AMOUNT) * (c.TAX/100) AS PAJAK_10,
SUM(a.TOTAL_TAX_CHARGE) AS PAJAK_BIAYA,
SUM(a.TOTAL_CHARGE) AS BIAYA_ANTAR,
(SUM(a.TOTAL_AMOUNT) * (c.TAX/100)) + SUM(a.TOTAL_AMOUNT) AS TOTAL
FROM T_POS_BILL a
JOIN M_POS b ON a.POS_CODE = b.POS_CODE
JOIN M_OUTLET c ON a.OUTLET_CODE = c.OUTLET_CODE
JOIN M_GLOBAL d ON b.POS_TYPE = d.CODE AND COND = 'POS_TYPE'
WHERE (a.DELIVERY_STATUS  IN (' ','CLS') OR a.DELIVERY_STATUS IS NULL) AND
a.OUTLET_CODE = $P{outletCode} AND
b.POS_TYPE =  $P{posType} 
AND a.trans_date BETWEEN $P{fromDate} AND  $P{toDate} 
GROUP BY a.TRANS_DATE,b.POS_TYPE,d.DESCRIPTION, a.OUTLET_CODE, c.OUTLET_NAME, c.ADDRESS_1 , c.ADDRESS_2, c.PHONE, c.TAX
ORDER BY a.TRANS_DATE ASC


]]>
	</queryString>
